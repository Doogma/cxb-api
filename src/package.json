{
  "name": "cxb-api",
  "version": "2.1.1",
  "description": "Example application for running a Node Express app on AWS Lambda using Amazon API Gateway.",
  "main": "lambda.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp package*json dist/",
    "dev": "vite-node --watch lambdas/api.local.ts",
    "start": "vite-node lambdas/api.local.ts",
    "test": "cross-env NODE_ENV=test mocha",
    "test:watch": "npm test -- -- -w",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint-fix": "eslint . --ext .ts,.js --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.614.0",
    "@aws-sdk/credential-providers": "^3.614.0",
    "@aws-sdk/lib-dynamodb": "^3.614.0",
    "@codegenie/serverless-express": "^4.3.10",
    "@smithy/types": "^3.3.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "source-map-support": "^0.5.19",
    "tslib": "^2.3.1",
    "vite-node": "^2.0.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "supertest": "^6.1.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
